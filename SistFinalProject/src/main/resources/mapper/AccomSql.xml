<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="sist.last.mapper.AccomMapperInter">

    <insert id="insertAccom" parameterType="AccomDto">
        insert into accom(business_id,accom_name,accom_category,accom_photo,accom_location) values (#{business_id},#{accom_name},#{accom_category},#{accom_photo},#{accom_location})
    </insert>
    
    <select id="getAllData" resultType="AccomDto">
    	select * from accom order by accom_num
    </select>
    
    <select id="getTotalCount" resultType="int">
        select count(*) from accom
    </select>
</mapper>